
	public static String m1(String s1)
	{
		String s2="";
			for(int i=0; i<=s1.length()-1;i++)
			{	
				boolean b1=true;
				for(int j=0;j<s2.length();j++)
				{
					if(s1.charAt(i)==s2.charAt(j))
					{
						b1 = false;
					}
				}
				if(b1)
				{
				s2=s2+s1.charAt(i);
				}
			}
			return s2;
	}
